<div class="container">
  <div class="row">
    <div class="col-sm-12 col-md-10 px-5 px-sm-0">

      <h2>配送先編集</h2>

      <div>
        <%= render 'address', address: @address %>
        <div>
          <%= form_with model: @address, local:true do |f| %>
            <%= f.submit "変更内容を保存", class:"btn btn-success" %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>