<div class="container">
  <div class="row">
    <div class="col-md-12 col-lg-10 px-5 mx-auto">

      <h2>配送先編集</h2>

      <div>
        <%= form_with model: @address, local:true do |f| %>
        <%= render 'address', f: f %>
        <div class="form-group text-center">
          <%= f.submit "変更内容を保存", class:"btn btn-success" %>
        </div>
        <% end %>
      </div>
    </div>
  </div>
</div>